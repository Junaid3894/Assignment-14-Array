/*Write a program to sort elements of an array of size 10. Take array values from the
user.*/
#include <stdio.h>
int main(){
	int arr[10];
	int min;
	int i,temp;
	for(i=0;i<5;i++)
	    {
	    // Input array elements from user
		printf("Enter the %d number :",i+1);
		scanf("%d",&arr[i]);
	    }
	// logic of sorting
	 for(int i=0;i<5-1;i++){
	 	for(int j=i+1;j<5;j++){
	 		if(arr[i]>arr[j])
                            {
	 			temp=arr[j];
	 			arr[j]=arr[i];
	 			arr[i]=temp;
	 	            }
	 	         }
	 	      }
// finally printing the sorted array	 	      
for(i=0;i<5;i++)
	    {
	    
		printf("%d\t",arr[i]);
	    }	 			 			
   return 0;
		}

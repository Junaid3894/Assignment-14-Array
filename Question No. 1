/* Write a program to calculate the sum of numbers stored in array of size 10.Take array values from the user.*/
#include <stdio.h>
int main(){
	int arr[10];
	int i;
	int sum=0;
	for(i=0;i<10;i++)
	    {
	    // Input array elements from user
		printf("Enter the %d number :",i+1);
		scanf("%d",&arr[i]);
	    // add each elements to sum variable
		sum+=arr[i];
		}
	printf("Sum of array is %d",sum);
		
		

	
   return 0;
}
